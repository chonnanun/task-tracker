              dateFormat: "yy-mm-dd"
            });

            $('body').removeClass().addClass('skin-blue sidebar-mini sidebar-collapse');



</script>


    <script type="text/javascript">
    updatedashboard();
    function updatedashboard() {
        var jqxhr = $.getJSON( "http://23.168.85.47:8081/task-tracker/api/dashboard", function() {
              console.log( "success" );
            })
              .done(function(data) {
                console.log( data );
                // console.log( data.id );
                $('#alltotal').html(data.alltotal);
                $('#allpending').html(data.allpending);
                $('#alldone').html(data.alldone);
                $('#latesttask').html("New task @" + data.allnewest.created_at + " | HWB: "+ data.allnewest.title);

              })
              .fail(function() {
                console.log( "error" );
              })
              .always(function() {
                console.log( "complete" );
              });

              window.setTimeout(updatedashboard, 10000);
          }
    

    </script>

