                                        <td style="display:none ">
                                        
  

                                        <script>
                                        function assigned(id) {
                                            event.preventDefault();

                                            
                                            now = $('#assigned-'+id).html();
                                            $('#now-user-'+id).hide();
                                            $('#new-user-'+id).show();


                                            // alert(now);


                                            // window.open("", "_blank", "toolbar=yes,scrollbars=yes,resizable=yes,top=100,left=100,width=400,height=600");
                                        }

                                         function doAssign(id) {
                                            newuser = $('#select-new-'+id).find(":selected").val();
                                            newusertxt = $('#select-new-'+id).find(":selected").text();
                                            $.ajax({
                                               type: "GET",
                                               url: "http://23.168.85.47:8081/task-tracker/assignTask/"+id+"/"+newuser,
                                               success: function(result){
                                                    $('#new-user-'+id).hide();
                                                    $('#now-user-'+id).html(newusertxt).show().addClass('lastupdated');

                                                   }
                                             });
                                         }

                                        $('.cancleassign').click(function(){
                                            $(this).parent().hide();
                                            $(this).parent().siblings().show();
                                        });


                                        function linkedit(id) {
                                            event.preventDefault();
                                            window.open("http://23.168.85.47:8081/task-tracker/task/view/"+id, "_blank", "toolbar=yes,scrollbars=yes,resizable=yes,top=100,left=100,width=1050,height=600");
                                        }


                                        </script>


                                        </td>

